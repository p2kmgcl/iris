{
  "name": "iris",
  "version": "0.0.14",
  "private": true,
  "scripts": {
    "analyze-bundle": "NODE_ENV=production webpack --profile --json --config webpack.config.ts > build/bundle-profile.json && webpack-bundle-analyzer build/bundle-profile.json build/app",
    "check-format": "prettier --list-different .",
    "format": "prettier --write .",
    "show-changelog": "conventional-changelog -p angular -r 2",
    "lint": "eslint --max-warnings 0 --fix app/**/*.ts app/**/*.tsx",
    "build": "NODE_ENV=production webpack --mode production --config webpack.config.ts",
    "start": "netlify dev",
    "start:development": "NODE_ENV=development webpack serve --mode development --config webpack.config.ts",
    "start:production": "NODE_ENV=production http-server -a 0.0.0.0 --port 9000 --gzip ./build/app/",
    "postinstall": "husky install"
  },
  "engines": {
    "node": ">=10.0.0",
    "yarn": ">=1.22.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run check-format && npm run lint"
    }
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.ts": "eslint --max-warnings 0 --fix",
    "*.tsx": "eslint --max-warnings 0 --fix"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.11.1",
    "@microsoft/microsoft-graph-client": "^2.2.1",
    "@microsoft/microsoft-graph-types": "^1.30.0",
    "idb": "^6.0.0",
    "node-fetch": "^2.6.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@types/compression-webpack-plugin": "^6.0.4",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/node": "^10.0.0",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/webpack": "^4.41.26",
    "@types/workbox-webpack-plugin": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "compression-webpack-plugin": "^7.1.2",
    "conventional-changelog-cli": "^2.1.1",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "eslint": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-server": "^0.12.3",
    "husky": "^5.0.0",
    "lint-staged": "^10.5.4",
    "netlify-cli": "^3.10.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "workbox-webpack-plugin": "^6.1.0"
  }
}
